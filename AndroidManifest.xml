<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lam.chesscaroandroid" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/anh"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.DarkActionBar">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".BackgroundSoundService"
            android:enabled="true">
        </service>

        <activity
            android:name=".Board_HumVsCom">
        </activity>

        <activity
            android:name=".Board_HumVsHum">
        </activity>

        <activity
            android:name=".Obj_Help">
        </activity>

        <activity
            android:name=".Obj_Info">
        </activity>

        <activity
            android:name=".Obj_Settings">
        </activity>

        <activity
            android:name=".Obj_HumVsCom">
        </activity>

        <activity
            android:name=".Obj_HumVsHum">
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>
